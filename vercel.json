{
    "buildCommand": "cd frontend && npm run build",
    "outputDirectory": "frontend/dist",
    "installCommand": "npm install --prefix frontend --legacy-peer-deps && npm install --prefix backend",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}